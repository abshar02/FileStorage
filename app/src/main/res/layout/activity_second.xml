<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="absharrafique.com.filestorage.SecondActivity">

    <EditText
        android:id="@+id/editText2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="24dp"
        android:layout_marginRight="28dp"
        android:inputType="textPersonName"/>

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/editText2"
        android:layout_alignStart="@+id/editText2"
        android:layout_below="@+id/editText2"
        android:textSize="18sp"
        android:layout_marginTop="23dp"
        android:text="Data Loaded Status........." />

    <Button
        android:id="@+id/btn6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_alignStart="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="58dp"
        android:layout_marginStart="58dp"
        android:layout_marginTop="29dp"
        android:onClick="loadFromInternalCache"
        android:text="Load From Internal Cache" />

    <Button
        android:id="@+id/btn7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/btn6"
        android:layout_alignStart="@+id/btn6"
        android:layout_below="@+id/btn6"
        android:layout_marginTop="27dp"
        android:onClick="loadFromExternalCache"
        android:text="Load From External Cache" />

    <Button
        android:id="@+id/btn8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn7"
        android:layout_alignLeft="@+id/btn7"
        android:layout_alignRight="@+id/btn7"
        android:layout_alignStart="@+id/btn7"
        android:layout_below="@+id/btn7"
        android:layout_marginTop="23dp"
        android:onClick="loadExternalPrivate"
        android:text="Load External Private" />

    <Button
        android:id="@+id/btn9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn8"
        android:layout_alignLeft="@+id/btn8"
        android:layout_alignRight="@+id/btn8"
        android:layout_alignStart="@+id/btn8"
        android:layout_below="@+id/btn8"
        android:layout_marginTop="27dp"
        android:onClick="loadExternalPublic"
        android:text="Load External Public" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/btn9"
        android:layout_alignLeft="@+id/btn9"
        android:layout_alignRight="@+id/btn9"
        android:layout_alignStart="@+id/btn9"
        android:layout_below="@+id/btn9"
        android:layout_marginTop="20dp"
        android:onClick="previous"
        android:text="Previous" />

</RelativeLayout>
